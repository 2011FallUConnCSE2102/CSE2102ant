<!-- Tests for the <not> condition. -->
<project version="2.0">

    <!-- An empty <or> condition -->
    <target name="empty">
        <assert>
            <not/>
        </assert>
    </target>

    <!-- The truth-table -->
    <target name="truth-table">
        <assert expected="false">
            <not><true/></not>
        </assert>
        <assert>
            <not><false/></not>
        </assert>
    </target>

    <!-- Too many nested conditions -->
    <target name="too-many-nested">
        <assert>
            <not><true/><false/></not>
        </assert>
    </target>

</project>